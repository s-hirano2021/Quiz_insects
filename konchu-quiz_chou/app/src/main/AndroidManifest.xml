<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chou">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcer_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcer_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Quiz01">
        <activity android:name="com.example.chou.ResultActivity"></activity>
        <activity android:name="com.example.chou.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>